{
  "id": "dev.simonbs.slack",
  "display_name": "Slack",
  "default_color": "orange",
  "icon": "https://github.com/simonbs/tapestry-plugins/blob/main/dev.simonbs.slack/icon.png?raw=true",
  "site": "https://slack.com",
  "site_placeholder": "https://slack.com",
  "item_style": "post",
  "oauth_authorize": "https://slack.com/oauth/authorize",
  "oauth_token": "https://slack.com/api/oauth.access",
  "oauth_type": "code",
  "oauth_code_key": "code",
  "oauth_scope": "channels:history+channels:read+users.profile:read+files:read+team:read",
  "oauth_grant_type": "authorization_code",
  "oauth_http_redirect": true,
  "needs_api_keys": true,
  "provides_attachments": true,
  "needs_verification": true,
  "verify_variables": true,
  "check_interval": 300
}
