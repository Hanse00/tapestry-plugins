{
  "id": "com.youtube",
  "display_name": "YouTube",
  "oauth_authorize": "https://accounts.google.com/o/oauth2/v2/auth",
  "oauth_token": "https://oauth2.googleapis.com/token",
  "oauth_type": "code",
