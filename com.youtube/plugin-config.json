{
  "id": "com.youtube",
  "display_name": "YouTube",
  "icon": "https://github.com/simonbs/tapestry-plugins/blob/main/com.youtube/icon.png?raw=true",
  "site": "https://www.googleapis.com",
  "site_placeholder": "https://www.googleapis.com",
  "item_style": "post",
  "oauth_authorize": "https://accounts.google.com/o/oauth2/v2/auth",
  "oauth_token": "https://oauth2.googleapis.com/token",
  "oauth_type": "code",
  "oauth_code_key": "code",
  "oauth_scope": "https://www.googleapis.com/auth/youtube&prompt=consent&access_type=offline&client_secret=",
  "oauth_grant_type": "authorization_code",
  "oauth_http_redirect": true,
  "needs_api_keys": true,
  "provides_attachments": true,
  "needs_verification": true,
  "verify_variables": true,
  "check_interval": 1800
}
